{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "jasmine": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "rules": {
    "quotes": 0,
    "no-var": 0,
    "vars-on-top": 0,
    "func-names": 0,
    "eqeqeq": 0,
    "consistent-return": 0,
    "no-multi-spaces": 0,
    "key-spacing": 0,
    "new-cap": 0,
    "jsx-quotes": 0,
    "semi": 0,
    "comma-dangle": 0
  }
}
