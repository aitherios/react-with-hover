{
  "name": "react-with-hover",
  "version": "1.2.0",
  "description": "Simple hover high order function.",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "clean": "rm -rf dist",
    "transpile": "babel src -D -d dist --ignore '__tests__'",
    "build": "npm run clean && npm run transpile && webpack --config webpack.config.js",
    "lint": "eslint ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/team-767/react-with-hover.git"
  },
  "keywords": [
    "react",
    "with-hover",
    "isomorphic",
    "inline-style",
    "high-order-component"
  ],
  "author": "Team Magneto",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/team-767/react-with-hover/issues"
  },
  "homepage": "https://github.com/team-767/react-with-hover#readme",
  "dependencies": {
    "recompose": ">= 0.26.0"
  },
  "peerDependencies": {
    "react": ">= 0.16"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.0",
    "jest-cli": "^22.1.4",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "prop-types": "^15.6.0",
    "webpack": "^3.10.0"
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "react",
      "react-dom",
      "react-addons-test-utils",
      "fbjs",
      "enzyme"
    ],
    "rootDir": "./src",
    "setupTestFrameworkScriptFile": "../tests.config.js"
  },
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-0"
    ]
  },
  "eslintConfig": {
    "extends": "airbnb",
    "env": {
      "browser": true,
      "node": true,
      "jasmine": true,
      "jest": true
    },
    "parser": "babel-eslint",
    "rules": {
      "semi": 0,
      "new-cap": 0
    }
  }
}
